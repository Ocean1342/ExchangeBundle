{
  "name": "ocean/exchange-bundle",
  "version": "0.1.0",
  "type": "symfony-bundle",
  "description": "Symfony exchange bundle ",
  "license": "proprietary",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ocean\\ExchangeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ocean\\ExchangeBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}